void Open_NaganakiShrine_Win_001()
{
	OPEN_MSG_WIN();
	MSG( QuickTravel );
	SEL_CHK_PAD( 6, 20 );
	int choice = SEL( NaganakiShrine );
	CLOSE_MSG_WIN();
	switch(choice)
	{
		case 0: // Town Map
			if(Chk_Access_NaganakiShrine_001_MAP())
			{
				FADE( 1, 0 );
				FADE_SYNC();
				CALL_MAP( 4 );
			}
			break;
		case 1: // Cancel
			break;
		default:
			break;
	}
}
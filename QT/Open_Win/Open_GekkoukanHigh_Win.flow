void Open_GekkoukanHigh_Win_001()
{
	OPEN_MSG_WIN();
	MSG( QuickTravel );
	SEL_CHK_PAD( 6, 20 );
	if( ( GET_MONTH() == 4 ) && ( GET_DAY_OF_MONTH() == 7 ) )
	{
		SET_MASK( 29 );
	}
	else
	{
		SET_MASK( 17 );
	}
	int choice = SEL( GekkoukanHigh );
	CLOSE_MSG_WIN();
	switch (choice)
	{
		case 1: // 1F, Faculty Office Hallway
			if(Chk_Access_GekkoukanHigh_001_007())
			{
				FADE( 2, 0 );
				FADE_SYNC();
				CALL_FIELD( 6, 7, 0, 0 );
			}
			break;
		case 2: // 1F, Laboratory Hallway
			if(Chk_Access_GekkoukanHigh_001_011())
			{
				FADE( 2, 0 );
				FADE_SYNC();
				CALL_FIELD( 6, 11, 2, 0 );
			}
			break;
		case 3: // 2F, Classroom Hallway
			if(Chk_Access_GekkoukanHigh_001_004())
			{
				FADE( 2, 0 );
				FADE_SYNC();
				CALL_FIELD( 6, 4, 1, 0 );
			}
			break;
		case 5: // Town Map
			if(Chk_Access_GekkoukanHigh_001_002())
			{
				if(Chk_Access_GekkoukanHigh_002_MAP())
				{
					FADE( 1, 0 );
					FADE_SYNC();
					CALL_MAP( 0 );
				}
			}
			break;
		default:
			break;
	}
}


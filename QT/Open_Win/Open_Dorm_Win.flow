void OpenDorm001()
{
	OPEN_MSG_WIN();
	MSG( QuickTravel );
	SEL_CHK_PAD( 6, 20 );
	SET_MASK( 46 );
	int choice = SEL( Dorm );
	CLOSE_MSG_WIN();
	switch (choice)
	{
		case 0: // 1F, Lounge
			if(ChkDorm001_003())
			{
				if(ChkDorm003_002())
				{
					FADE( 2, 0 );
					FADE_SYNC();
					CALL_FIELD( 7, 2, 1, 0 );
				}
			}
			break;
		case 3: // 3F, Hallway
			if(ChkDorm001_003())
			{
				if(ChkDorm003_004())
				{
					FADE( 2, 0 );
					FADE_SYNC();
					CALL_FIELD( 7, 4, 1, 0 );
				}
			}
			break;
		case 4: // Meeting Room
			if(ChkDorm001_003())
			{
				if(ChkDorm003_004())
				{
					if(ChkDorm004_005())
					{
						if(ChkDorm005_006())
						{
							FADE( 2, 0 );
							FADE_SYNC();
							CALL_FIELD( 7, 6, 0, 0 );
						}
					}
				}
			}
			break;
		case 6: // Town Map
			if(ChkDorm001_003())
			{
				if(ChkDorm003_002())
				{
					if(ChkDorm002_009())
					{
						if(ChkDorm009_MAP())
						{
							FADE( 1, 0 );
							FADE_SYNC();
							CALL_MAP ( 5 );

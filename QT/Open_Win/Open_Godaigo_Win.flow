void Open_Godaigo_Win_002()
{
	OPEN_MSG_WIN();
	MSG( QuickTravel );
	SEL_CHK_PAD( 6, 20 );
	SET_MASK( 1 );
	int choice = SEL( Godaigo );
	CLOSE_MSG_WIN();
	switch(choice)
	{
		case 1: // 2F, Hallway
			if(Chk_Access_Godaigo_002_003())
			{
				FADE( 2, 0 );
				FADE_SYNC();
				CALL_FIELD( 14, 3, 1, 0 );
			}
			break;
		case 2: // 3F, Hallway
			if(Chk_Access_Godaigo_002_003())
			{
				if(Chk_Access_Godaigo_003_004())
				{
					FADE( 2, 0 );
					FADE_SYNC();
					CALL_FIELD( 14, 4, 0, 0 );
				}
			}
			break;
		default:
			break;
	}
}


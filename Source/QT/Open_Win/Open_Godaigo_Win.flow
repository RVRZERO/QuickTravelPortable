void OpenGoda002()
{
	OPEN_MSG_WIN();
	MSG( QuickTravel );
	SEL_CHK_PAD( 6, 20 );
	int choice = SEL( Godaigo );
	CLOSE_MSG_WIN();
	switch(choice)
	{
		case 0: // 1F Lobby
			FADE( 2, 0 );
			FADE_SYNC();
			CALL_FIELD( 14, 2, 3, 0 );
			break;
		case 1: // 2F, Hallway
			if(ChkGoda002_003())
			{
				FADE( 2, 0 );
				FADE_SYNC();
				CALL_FIELD( 14, 3, 1, 0 );
			}
			break;
		case 2: // 3F, Hallway
			if(ChkGoda002_003())
			{
				if(ChkGoda003_004())
				{
					FADE( 2, 0 );
					FADE_SYNC();
					CALL_FIELD( 14, 4, 0, 0 );
				}
			}
			break;
		case 3: // Cancel
			break;
		default:
			break;
	}
}

